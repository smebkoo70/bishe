@model IEnumerable<bishe1.Entity.assess1>
@using bishe1.Entity;
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLogonLayout.cshtml";
}

<h3>这里显示所有未处理的评估信息.</h3>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.loginID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.auditID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.auditstatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.result)
        </th>
        <th>
            操作
        </th>
        <th></th>
    </tr>

    @foreach (assess1 assessInfo in ViewBag.AssessList)
    {
        <tr>
            <td>@assessInfo.id</td>

            <td>@assessInfo.loginID</td>

            <td>@assessInfo.auditID</td>

            <td>@assessInfo.type</td>

            <td>@assessInfo.date</td>

            <td>@assessInfo.auditstatus</td>

            <td>@assessInfo.result</td>


            <td>
                <a href="/BackAssess/AssessModify/@assessInfo.id">修改</a>

            </td>
        </tr>
    }

</table>

<h3>这里显示所有已处理的评估信息，不能修改了</h3>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.loginID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.auditID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.auditstatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.result)
        </th>
        <th>
            操作
        </th>
        <th></th>
    </tr>

    @foreach (assess1 assessInfo in ViewBag.AssessList2)
    {
        <tr>
            <td>@assessInfo.id</td>

            <td>@assessInfo.loginID</td>

            <td>@assessInfo.auditID</td>

            <td>@assessInfo.type</td>

            <td>@assessInfo.date</td>

            <td>@assessInfo.auditstatus</td>

            <td>@assessInfo.result</td>


            <td>
                <a href="/BackAssess/AssessDetails/@assessInfo.id">查看</a>

            </td>
        </tr>
    }

</table>